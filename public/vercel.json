{
    "rewrites": [
        {
            "source": "/host",
            "destination": "/host.html"
        },
        {
            "source": "/player",
            "destination": "/player.html"
        },
        {
            "source": "/creator",
            "destination": "/creator.html"
        },
        {
            "source": "/painel",
            "destination": "/dashboard.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}